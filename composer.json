{
  "name": "uploadcare/uploadcare-wordpress",
  "require": {
    "php": ">=7.4 || >=8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-dom": "*",
    "uploadcare/uploadcare-php": "^3.1",
    "symfony/dom-crawler": "^5.4",
    "twig/twig": "^2.13|^3.0"
  },
  "autoload": {
    "classmap": ["includes/", "admin/"]
  },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
  "require-dev": {
    "phpunit/phpunit": "^7||^9",
    "symfony/var-dumper": "^4.4||^5",
    "yoast/phpunit-polyfills": "^2.0"
  }
}
